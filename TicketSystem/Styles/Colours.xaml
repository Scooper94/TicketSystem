<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--https://coolors.co/2b2d42-8d99ae-edf2f4-ef233c-d90429-->
    <Color x:Key="BackgroundLight">#edf2f4</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="BackgroundDark">#2B2D42</Color>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}" />

    <Color x:Key="BackgroundHighlight">#D90429</Color>
    <SolidColorBrush x:Key="BackgroundHighlightBrush" Color="{StaticResource BackgroundHighlight}" />

    <Color x:Key="TextLight">#edf2f4</Color>
    <SolidColorBrush x:Key="TextLightBrush" Color="{StaticResource TextLight}" />

    <Color x:Key="TextDark">#2B2D42</Color>
    <SolidColorBrush x:Key="TextDarkBrush" Color="{StaticResource TextDark}" />

    <Color x:Key="TextHighlight">#D90429</Color>
    <SolidColorBrush x:Key="TextHighlightBrush" Color="{StaticResource TextHighlight}" />
</ResourceDictionary>